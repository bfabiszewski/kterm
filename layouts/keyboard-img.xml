        <mod1 display="image:img/iacute.png" action="í" />
        <mod2 display="image:img/ciacute.png" action="Í" />
      </key>
      <key obey-caps="true">
        <default display="image:img/d.png" action="d" />
        <shifted display="image:img/cd.png" action="D" />
        <mod1 display="image:img/icirc.png" action="î" />
        <mod2 display="image:img/cicirc.png" action="Î" />
      </key>
      <key obey-caps="true">
        <default display="image:img/f.png" action="f" />
        <shifted display="image:img/cf.png" action="F" />
        <mod1 display="image:img/iuml.png" action="ï" />
        <mod2 display="image:img/ciuml.png" action="Ï" />
      </key>
      <key obey-caps="true">
        <default display="image:img/g.png" action="g" />
        <shifted display="image:img/cg.png" action="G" />
        <mod1 display="image:img/ntilde.png" action="ñ" />
        <mod2 display="image:img/cntilde.png" action="Ñ" />
      </key>
      <key obey-caps="true">
        <default display="image:img/h.png" action="h" />
        <shifted display="image:img/ch.png" action="H" />
        <mod1 display="image:img/ograve.png" action="ò" />
        <mod2 display="image:img/cograve.png" action="Ò" />
      </key>
      <key obey-caps="true">
        <default display="image:img/j.png" action="j" />
        <shifted display="image:img/cj.png" action="J" />
        <mod1 display="image:img/oacute.png" action="ó" />
        <mod2 display="image:img/coacute.png" action="Ó" />
      </key>
      <key obey-caps="true">
        <default display="image:img/k.png" action="k" />
        <shifted display="image:img/ck.png" action="K" />
        <mod1 display="image:img/otilde.png" action="õ" />
