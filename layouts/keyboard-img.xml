<?xml version="1.0" encoding="UTF-8"?>
<!-- image-only layout for kindle touch -->
<!-- Patrick de Groot - www.chinesegadgets.net -->
<!-- modified for kindle by baf -->
<!-- modified for PW by cubemike99 -->
<!-- modified for layout by DT -->
<keyboard>
  <options>
    <!-- not yet implemented -->
  </options>
  <layout id="kt image keyboard">
    <row>
      <key>
        <default display="image:img/esc.png" action="escape" />
      </key>
      <key>
        <default display="image:img/1.png" action="1" />
        <shifted display="image:img/exclamation.png" action="!" />
        <mod1 display="image:img/f1.png" action="f1" />
        <mod2 display="image:img/sup1.png" action="¹" />
      </key>
      <key>
        <default display="image:img/2.png" action="2" />
        <shifted display="image:img/at.png" action="@" />
        <mod1 display="image:img/f2.png" action="f2" />
        <mod2 display="image:img/sup2.png" action="²" />
      </key>
      <key>
        <default display="image:img/3.png" action="3" />
        <shifted display="image:img/hash.png" action="#" />
        <mod1 display="image:img/f3.png" action="f3" />
        <mod2 display="image:img/euro.png" action="€" />
      </key>
      <key>
        <default display="image:img/4.png" action="4" />
        <shifted display="image:img/dollar.png" action="$" />
        <mod1 display="image:img/f4.png" action="f4" />
        <mod2 display="image:img/pound.png" action="£" />
      </key>
      <key>
        <default display="image:img/5.png" action="5" />
        <shifted display="image:img/percent.png" action="%" />
        <mod1 display="image:img/f5.png" action="f5" />
        <mod2 display="image:img/yen.png" action="¥" />
      </key>
      <key>
        <default display="image:img/6.png" action="6" />
        <shifted display="image:img/caret.png" action="^" />
        <mod1 display="image:img/f6.png" action="f6" />
        <mod2 display="image:img/cent.png" action="¢" />
      </key>
      <key>
        <default display="image:img/7.png" action="7" />
        <shifted display="image:img/amp.png" action="&amp;" />
        <mod1 display="image:img/f7.png" action="f7" />
        <mod2 display="image:img/iexcl.png" action="¡" />
      </key>
      <key>
        <default display="image:img/8.png" action="8" />
        <shifted display="image:img/asterisk.png" action="*" />
        <mod1 display="image:img/f8.png" action="f8" />
        <mod2 display="image:img/iquest.png" action="¿" />
      </key>
      <key>
        <default display="image:img/9.png" action="9" />
        <shifted display="image:img/roundbracketleft.png" action="(" />
        <mod1 display="image:img/f9.png" action="f9" />
        <mod2 display="image:img/para.png" action="¶" />
      </key>
      <key>
        <default display="image:img/0.png" action="0" />
        <shifted display="image:img/roundbracketright.png" action=")" />
        <mod1 display="image:img/f10.png" action="f10" />
        <mod2 display="image:img/sect.png" action="§" />
      </key>
      <key>
        <default display="image:img/minus.png" action="-" />
        <shifted display="image:img/underscore.png" action="_" />
        <mod1 display="image:img/f11.png" action="f11" />
        <mod2 display="image:img/micro.png" action="µ" />
      </key>
      <key>
        <default display="image:img/equals.png" action="=" />
        <shifted display="image:img/plus.png" action="+" />
        <mod1 display="image:img/f12.png" action="f12" />
        <mod2 display="image:img/deg.png" action="°" />
      </key>
      <key width="1500" fill="true">
        <default display="image:img/back.png" action="backspace" />
        <shifted display="image:img/del.png" action="delete" />
      </key>
    </row>
    <row>
      <key width="1500" obey-caps="true" fill="true">
        <default display="image:img/tab.png" action="tab" />
      </key>
      <key obey-caps="true">
        <default display="image:img/q.png" action="q" />
        <shifted display="image:img/cq.png" action="Q" />
        <mod1 display="image:img/agrave.png" action="à" />
        <mod2 display="image:img/cagrave.png" action="À" />
      </key>
      <key obey-caps="true">
