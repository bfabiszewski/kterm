        <mod2 display="image:img/cacirc.png" action="Â" />
      </key>
      <key width="1000" obey-caps="true">
        <default display="image:img/t.png" action="t" />
        <shifted display="image:img/ct.png" action="T" />
        <mod1 display="image:img/auml.png" action="ä" />
        <mod2 display="image:img/cauml.png" action="Ä" />
      </key>
      <key obey-caps="true">
        <default display="image:img/y.png" action="y" />
        <shifted display="image:img/cy.png" action="Y" />
        <mod1 display="image:img/aring.png" action="å" />
        <mod2 display="image:img/caring.png" action="Å" />
      </key>
      <key obey-caps="true">
        <default display="image:img/u.png" action="u" />
        <shifted display="image:img/cu.png" action="U" />
        <mod1 display="image:img/aelig.png" action="æ" />
        <mod2 display="image:img/caelig.png" action="Æ" />
      </key>
      <key obey-caps="true">
        <default display="image:img/i.png" action="i" />
        <shifted display="image:img/ci.png" action="I" />
        <mod1 display="image:img/ccedil.png" action="ç" />
        <mod2 display="image:img/cccedil.png" action="Ç" />
      </key>
      <key obey-caps="true">
        <default display="image:img/o.png" action="o" />
        <shifted display="image:img/co.png" action="O" />
        <mod1 display="image:img/egrave.png" action="è" />
        <mod2 display="image:img/cegrave.png" action="È" />
      </key>
      <key obey-caps="true">
        <default display="image:img/p.png" action="p" />
        <shifted display="image:img/cp.png" action="P" />
        <mod1 display="image:img/eacute.png" action="é" />
        <mod2 display="image:img/ceacute.png" action="É" />
      </key>
      <key>
        <default display="image:img/squarebracketleft.png" action="[" />
        <shifted display="image:img/curlybracketleft.png" action="{" />
        <mod1 display="image:img/ecirc.png" action="ê" />
        <mod2 display="image:img/cecirc.png" action="Ê" />
      </key>
      <key>
