        <shifted display="image:img/quot.png" action="&quot;" />
        <mod1 display="image:img/oslash.png" action="ø" />
        <mod2 display="image:img/coslash.png" action="Ø" />
      </key>
      <key width="2000">
        <default display="image:img/return.png" action="return" />
      </key>
    </row>
    <row>
      <key width="1500" fill="true">
        <default display="image:img/shift.png" action="modifier:shift" />
