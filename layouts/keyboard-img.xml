      </key>
      <key obey-caps="true">
        <default display="image:img/v.png" action="v" />
        <shifted display="image:img/cv.png" action="V" />
        <mod1 display="image:img/ucirc.png" action="û" />
        <mod2 display="image:img/cuuml.png" action="Ü" />
      </key>
      <key obey-caps="true">
        <default display="image:img/b.png" action="b" />
        <shifted display="image:img/cb.png" action="B" />
        <mod1 display="image:img/uuml.png" action="ü" />
