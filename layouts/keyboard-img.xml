        <shifted display="image:img/curlybracketright.png" action="}" />
        <mod1 display="image:img/euml.png" action="ë" />
        <mod2 display="image:img/ceuml.png" action="Ë" />
      </key>
      <key>
        <default display="image:img/backslash.png" action="\" />
        <shifted display="image:img/pipe.png" action="|" />
      </key>
    </row>
    <row>
      <key width="1500" fill="true">
        <default display="image:img/capslk.png" action="modifier:caps" />
      </key>
      <key obey-caps="true">
        <default display="image:img/a.png" action="a" />
        <shifted display="image:img/ca.png" action="A" />
        <mod1 display="image:img/igrave.png" action="ì" />
